{
    "name": "PIU UCS Page Append",
    "description": "A Chrome Extension for Pump It Up Ucs Page",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "http://www.piugame.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["http://www.piugame.com/*wr_id*"],
            "js": ["insert.js"],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon_16.png"
    }
}